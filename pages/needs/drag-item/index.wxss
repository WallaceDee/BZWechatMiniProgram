.label{
  display: block;
  font-size: 14px;
  color: #353839;
  line-height: 38px;
  text-overflow: ellipsis;
  overflow: hidden;
  white-space: nowrap;
  text-align: center;
  padding: 0 10px;
}
.inner{
  background-color: #F8F7F7;
  margin: 7px 4px;
  border-radius: 38px;
}
  .delete-btn {
    position: absolute;
    border: 1px solid #c3c3c3;
    width: 14px;
    height: 14px;
    display: block;
    text-align: center;
    border-radius: 50%;
    right:0;
    top: 5px;
    background-color: #fff;
  
}
.delete-btn:before ,.delete-btn:after {
  content: '';
  display: block;
  width: 1px;
  height: 50%;
  background-color: #c3c3c3;
  position: absolute;
  transform: translateX(-50%) translateY(-50%) rotate(45deg);
  left: 50%;
  top: 50%;
}
.delete-btn:after {
  transform: translateX(-50%) translateY(-50%) rotate(135deg);
}