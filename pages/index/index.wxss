.container {
  height: 100%;
  min-height: 100%;
  display: flex;
  flex-direction: column;
  box-sizing: border-box;
}
.nav-tabs {
  justify-content: flex-start !important;
}
.nav-tab {
  font-size: 18px;
}
.nav-tab .i-tabs-tab-title {
  font-size: 18px;
  color: #353839;
  margin-left: 20px;
  margin-right: 20px;
}
.nav-tab .i-tabs-tab-title-current {
  font-weight: bold;
}
